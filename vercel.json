{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*.ts"]
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/dist/api.js" },
      { "src": "/", "dest": "/dist/home.js" }
    ]
  }
  